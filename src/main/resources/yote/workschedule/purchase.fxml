                <Font name="Arial" size="13.0" />
            </font>
        </Button>
        <Text fill="#775b3d" layoutX="164.0" layoutY="267.0" strokeType="OUTSIDE" strokeWidth="0.0" text="DATE : ">
            <font>
                <Font name="Arial" size="14.0" />
            </font>
        </Text>
        <Text fill="#775b3d" layoutX="164.0" layoutY="305.0" strokeType="OUTSIDE" strokeWidth="0.0" text="TIME : ">
            <font>
                <Font name="Arial" size="14.0" />
            </font>
        </Text>
        <Label fx:id="programPurchaseLabel" layoutX="117.0" layoutY="205.0" text="programPurchaseLabel" textFill="#a98a68">
            <font>
                <Font name="Arial" size="18.0" />
            </font>
        </Label>
        <Label fx:id="datePurchaseLabel" layoutX="213.0" layoutY="254.0" text="datePurchaseLabel" textFill="#a98a68">
            <font>
                <Font name="Arial" size="14.0" />
            </font>
        </Label>
        <Label fx:id="timePurchaseLabel" layoutX="213.0" layoutY="292.0" text="timePurchaseLabel" textFill="#a98a68">
            <font>
                <Font name="Arial" size="14.0" />
            </font>
        </Label>
        <Label fx:id="pricePurchaseLabel" layoutX="538.0" layoutY="205.0" text="pricePurchaseLabel" textFill="#a98a68">
            <font>
                <Font name="Arial" size="18.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
